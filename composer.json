{
    "name": "trilobit-gmbh/contao-calculator-bundle",
    "type": "contao-bundle",
    "description": "Contao 4 calculator bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "trilobit GmbH",
            "homepage": "https://github.com/trilobit-gmbh"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "contao/core-bundle": "~4.9",
        "symfony/expression-language": "^3.4 || ^4.0 || ^5.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.1"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Trilobit\\ContaoCalculator\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Trilobit\\ContaoCalculator\\ContaoManager\\Plugin"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "contao/manager-plugin": true
        }
    }
}
